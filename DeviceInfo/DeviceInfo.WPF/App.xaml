<Application x:Class="DeviceInfo.WPF.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:DeviceInfo.WPF"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <SolidColorBrush x:Key="CommandBarBackgroundColor" Color="#00508D" />
        <SolidColorBrush x:Key="CommandBarTextColor" Color="White" />

        <SolidColorBrush x:Key="DefaultTitleBarBackgroundColor" Color="#00508D" />
        <SolidColorBrush x:Key="DefaultTitleBarTextColor" Color="White" />

    </Application.Resources>
</Application>
